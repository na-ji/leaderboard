{
  "name": "pogo-leaderboard",
  "version": "5.5.1",
  "private": true,
  "scripts": {
    "build": "npm run i18n:compile && npm run build:next && npm run build:server",
    "build:next": "cross-env NODE_ENV=production next build",
    "build-storybook": "build-storybook",
    "build:server": "tsc --project tsconfig.server.json",
    "chromatic": "chromatic --exit-zero-on-changes",
    "commit": "git-cz",
    "dev": "cross-env NODE_ENV=development ts-node --project tsconfig.server.json server/index.ts",
    "dev:next-only": "cross-env NODE_ENV=development PORT=4000 NEXTAUTH_URL=http://localhost:4000 next dev",
    "i18n:compile": "formatjs compile-folder --ast src/i18n/lang src/i18n/compiled-lang",
    "i18n:extract": "formatjs extract 'src/**/*.{js,ts,tsx}' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file src/i18n/lang/en.json",
    "i18n:sync": "ts-node --project tsconfig.server.json server/synchroniseTranslationFile.ts",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "node-config-ts",
    "prebuild": "node-config-ts && npm run schema:create",
    "prepare": "husky install",
    "prettier": "prettier --write --ignore-unknown 'src/**/*'",
    "schema:create": "cross-env NODE_ENV=production ts-node --project tsconfig.server.json server/create-schema.ts",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "storybook": "start-storybook -p 7349"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "formatjs"
    ],
    "extends": [
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "formatjs/enforce-id": [
        "error"
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "arrowParens": "always"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.gitTag} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "preset": "angular"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 7"
  },
  "volta": {
    "node": "16.17.0",
    "npm": "8.19.2"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^28.1.1",
    "@ag-grid-community/core": "^28.1.1",
    "@ag-grid-community/react": "^28.1.1",
    "@headlessui/react": "^1.7.3",
    "@next-auth/sequelize-adapter": "^1.0.6",
    "clsx": "^1.2.1",
    "cron": "^2.1.0",
    "discord-api-types": "^0.37.14",
    "mysql2": "^2.3.3",
    "next": "12.3.1",
    "next-auth": "^4.18.5",
    "next-transpile-modules": "^9.1.0",
    "node-config-ts": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^6.1.2",
    "sequelize": "^6.24.0",
    "sharp": "^0.31.1",
    "swr": "^1.3.0",
    "tslog": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@formatjs/cli": "^5.1.3",
    "@mdx-js/preact": "^2.1.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/cron": "^2.0.0",
    "@types/node": "^18.11.5",
    "@types/react": "17.0.44",
    "@types/react-dom": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.11.2",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "cz-conventional-commit": "^1.0.6",
    "eslint": "8.26.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^4.3.4",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "sequelize-cli": "^6.5.2",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4",
    "vite": "^3.1.6",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "^3.5.1",
    "webpack-stats-plugin": "^1.1.0"
  }
}
